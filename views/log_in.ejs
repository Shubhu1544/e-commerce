  <script>
    $(document).ready(function () {
      $('#show-email').hide();
      $('#show-pass').hide();

      var email_err = true;
      var pass_err = true;

      

      $('#email').keyup(function () {
        email_error();
      });
      function email_error() {
        var email_val = $('#email').val();

        if (email_val.length == "") {
          $('#show-email').show();
          $('#show-email').html("**plzz fill your email id**");
          $('#show-email').css('color', 'red');
          $('#show-email').focus();
          email_err = false;
          return false;
        }
        else {
          $('#show-email').hide();
          return true;
        }
      }

      $('#pass').keyup(function () {
        pass_error();
      });
      function pass_error() {
        var pass_val = $('#pass').val();
        if (pass_val.length == "") {
          $('#show-pass').show();
          $('#show-pass').html("**plzz fill the password name**");
          $('#show-pass').css('color', 'red');
          $('#show-pass').focus();
          pass_err = false;
          return false;
        }
        else if ((pass_val.length <= 3) || (pass_val.length >= 15)) {
          $('#show-pass').show();
          $('#show-pass').html("**plzz your password must be 3 and 15**");
          $('#show-pass').css('color', 'red');
          $('#show-pass').focus();
          pass_err = false;
          return false;
        }
        else {
          $('#show-pass').hide();
          return true;
        }
      }

      $('#submit_btn').click(function () {
        email_err = true;
        pass_err = true;

        email_error();
        pass_error();

        if (email_error() == true && pass_error() == true) {
          // console.log( true)
          return true;
        }
        else {
          // console.log( false)
          return false;
        }
      });

    });

  </script>
